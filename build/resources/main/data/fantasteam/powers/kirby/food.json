      "change": 4,
      "operation": "set"
    },
    "item_condition": {
      "type": "origins:ingredient",
      "ingredient": [
        {
          "tag": "fantasteamorigins:striderfood"
        }
      ]
    }
  },
  "milk": {
    "type": "origins:action_on_item_use",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_food_eaten",
      "change": 0,
      "operation": "set"
    },
    "item_condition": {
      "type": "origins:ingredient",
      "ingredient": [
        {
          "item": "milk_bucket"
        }
      ]
    }
  },
  "effect_meat": {
    "type": "origins:action_over_time",
    "interval": 1,
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type": "origins:resource",
        "resource": "*:*_food_eaten",
        "comparison": "==",
        "compare_to": 1
      },
      "if_action": {
        "type": "origins:apply_effect",
        "effect": {
          "effect": "strength",
          "duration": 5,
          "amplifier": 0,
          "show_particles": false,
          "show_icon": false
        }
      },
      "else_action": {
        "type": "origins:nothing"
      }
    }
  },
  "effect_fish": {
    "type": "origins:action_over_time",
