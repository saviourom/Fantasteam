{
  "type": "origins:multiple",
  "food_eaten": {
    "type": "origins:resource",
    "min": 0,
    "max": 4
  },
  "eat_beef": {
    "type": "origins:action_on_item_use",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_food_eaten",
      "change": 1,
      "operation": "set"
    },
    "item_condition": {
      "type": "origins:ingredient",
      "ingredient": [
        {
          "item": "cooked_mutton"
        },
        {
          "item": "cooked_porkchop"
        },
        {
          "item": "cooked_beef"
        }
      ]
    }
  },
  "eat_fish": {
    "type": "origins:action_on_item_use",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_food_eaten",
      "change": 2,
      "operation": "set"
    },
    "item_condition": {
      "type": "origins:ingredient",
      "ingredient": [
        {
          "tag": "minecraft:fishes"
        },
        {
          "tag": "fantasteamorigins:seafood"
        }
      ]
    }
  },
  "eat_nether": {
    "type": "origins:action_on_item_use",
