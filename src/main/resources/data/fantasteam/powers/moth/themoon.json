{
  "type": "origins:multiple",

  "new_moon": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type": "origins:predicate",
        "predicate": "fantasteam:moon_at_night/is_new_moon"
      },
      "if_action": {
        "type": "origins:grant_power",
        "power": "fantasteam:misc/new_moon",
        "source": "fantasteam:misc"
      },
      "else_action": {
        "type": "origins:revoke_power",
        "power": "fantasteam:misc/new_moon",
        "source": "fantasteam:misc"
      }
    },
    "interval": 1
  },

